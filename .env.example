# OpenAI API Configuration (legacy - still supported)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Replicate API Configuration (Server-side only - DO NOT use VITE_ prefix)
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Application Configuration
VITE_APP_NAME=BrandScene

# Development Configuration
NODE_ENV=development

# Production Configuration (for CORS)
FRONTEND_URL=https://your-domain.vercel.app

# Optional: Choose AI Provider (openai or replicate)
VITE_AI_PROVIDER=replicate